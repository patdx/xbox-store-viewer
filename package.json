{
	"name": "xbox-store-viewer",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "nx exec -- wrangler types && react-router build",
		"deploy": "nx exec -- wrangler deploy",
		"dev": "react-router dev",
		"start": "nx exec -- wrangler dev",
		"typecheck": "tsc",
		"typegen": "wrangler types",
		"update-react": "pnpm install --save-exact react@rc react-dom@rc",
		"format": "prettier --write .",
		"scn:add": "pnpx shadcn-ui@latest add"
	},
	"dependencies": {
		"@radix-ui/react-slot": "1.2.3",
		"class-variance-authority": "0.7.1",
		"clsx": "2.1.1",
		"isbot": "5.1.30",
		"lodash-es": "4.17.21",
		"lucide-react": "0.542.0",
		"ofetch": "1.4.1",
		"react": "rc",
		"react-dom": "rc",
		"react-error-boundary": "6.0.0",
		"react-router": "^7.7.1",
		"tailwind-merge": "3.3.1",
		"tailwindcss-animate": "1.0.7",
		"valibot": "1.1.0"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "1.10.2",
		"@cloudflare/workers-types": "4.20250726.0",
		"@react-router/dev": "^7.7.1",
		"@react-router/fs-routes": "7.8.2",
		"@tailwindcss/postcss": "4.1.12",
		"@tailwindcss/vite": "4.1.12",
		"@types/lodash-es": "4.17.12",
		"@types/react": "npm:types-react@rc",
		"@types/react-dom": "npm:types-react-dom@rc",
		"nx": "21.4.1",
		"postcss": "8.5.6",
		"prettier": "3.6.2",
		"prettier-plugin-tailwindcss": "0.6.14",
		"tailwindcss": "4.1.12",
		"typescript": "5.9.2",
		"unplugin-auto-import": "19.3.0",
		"vite": "7.1.3",
		"vite-tsconfig-paths": "5.1.4",
		"wrangler": "4.26.1"
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"better-sqlite3"
		],
		"overrides": {
			"@types/react": "$@types/react",
			"@types/react-dom": "$@types/react-dom"
		}
	},
	"packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b",
	"nx": {
		"targets": {
			"deploy": {
				"dependsOn": [
					"build"
				]
			},
			"start": {
				"dependsOn": [
					"build"
				]
			}
		}
	}
}
