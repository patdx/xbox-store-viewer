{
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"editor.tabSize": 2,
	"files.associations": {
		"*.css": "tailwindcss",
		"_headers": "plaintext"
	},
	"typescript.tsdk": "node_modules/typescript/lib",
	"typescript.enablePromptUseWorkspaceTsdk": true,
	"tailwindCSS.classFunctions": ["tw", "clsx", "cva"],
	"tailwindCSS.experimental.configFile": "app/app.css",
	"cSpell.words": ["Stylesheet", "workerd"]
}
